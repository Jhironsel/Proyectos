	<subDataset name="ds_Estado_Conteo" uuid="1c1a228f-74a4-4548-b618-40678ab041e6">
		<queryString>
			<![CDATA[select e.estado, count(*)
from estudiantes e
group by e.ESTADO]]>
		</queryString>
		<field name="ESTADO" class="java.lang.Integer"/>
		<field name="COUNT" class="java.lang.Integer"/>
	</subDataset>
	<parameter name="logo" class="java.io.InputStream"/>
	<queryString>
		<![CDATA[select e.MATRICULA,
       e.NOMBRES,
       e.APELLIDOS,
       (select 'Hora de Inicio: '||substring(HORA_INICIO from 1 for 8)
       ||' Hora Final: '|| substring(HORA_FINAL from 1 for 8)
       from Tandas t
       where t.ID_Tanda = e.Id_tanda)as tanda,
       e.ESTADO,(select trim(
	case lunes
            	when 1 then 'Lunes '
		else trim('')
	end||
	case martes
		when 1 then 'Martes '
		else trim('')
	end||
       	case miercoles
            	when 1 then 'Miercoles '
		else trim('')
      	end||
       	case jueves
            	when 1 then 'Jueves '
		else trim('')
       	end||
       	case viernes
            	when 1 then 'Viernes '
		else trim('')
       	end||
       	case sabados
           	when 1 then 'Sabados '
		else trim('')
       	end||
          case domingos
            	when 1 then 'Domingos '
		else trim('')
	end)
       from Tandas t
       where t.ID_Tanda = e.Id_tanda)as dias
from Estudiantes e]]>
	</queryString>
